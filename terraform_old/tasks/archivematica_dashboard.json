[
  {
    "essential": true,
    "memory": 512,
    "name": "archivematica_dashboard",
    "cpu": 256,
    "image": "760097843905.dkr.ecr.eu-west-1.amazonaws.com/archivematica_dashboard:1.14",
    "workingDirectory": "/code",
    "environment" : [
        { "name" : "DJANGO_PORT", "value" : "8002"},
        {"name": "DJANGO_STATIC_ROOT", "value": "/data/archivematica/dashboard-static"},
        {"name": "SS_DB_URL", "value": "mysql://archivematica_db_user:archivematica_password@archivematicadashboarddb.cydsvlj5npwq.eu-west-1.rds.amazonaws.com:3306/archivematicadashboarddb?init_command=SET sql_mode='STRICT_TRANS_TABLES'"}
    ],
    "MountPoints": [
      {
        "ContainerPath": "/data/archivematica",
        "SourceVolume": "archivematica-ebs-volume"
      },
      {
        "ContainerPath": "/var/archivematica/sharedDirectory",
        "SourceVolume": "archivematica-ebs-volume-pipeline-sharedDirectory"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8002,
        "hostPort": 8002
      }
    ]
  }
]
