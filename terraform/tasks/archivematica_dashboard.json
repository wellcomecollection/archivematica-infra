[
  {
    "essential": true,
    "memory": 512,
    "name": "archivematica_dashboard",
    "cpu": 256,
    "image": "760097843905.dkr.ecr.eu-west-1.amazonaws.com/archivematica_dashboard:1.09",
    "workingDirectory": "/code",
    "environment" : [
        { "name" : "DJANGO_PORT", "value" : "8002" },
        {"name": "DJANGO_STATIC_ROOT", "value": "/data/archivematica/dashboard-static"}
    ],
    "MountPoints": [
      {
        "ContainerPath": "/data/archivematica",
        "SourceVolume": "archivematica-ebs-volume"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8002,
        "hostPort": 8002
      }
    ]
  }
]